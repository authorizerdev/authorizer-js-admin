{
  "extends": "@antfu",
  "rules": {
      "no-console": "off",
      "no-tabs": "off",
      "@typescript-eslint/consistent-type-imports": "off",
      "@typescript-eslint/ban-ts-comment": "off",
      "semi": "off",
      "@typescript-eslint/semi": "off"
  },
  "ignorePatterns": [
      "node_modules",
      "lib",
      ".github",
      "**.js"
  ]
}